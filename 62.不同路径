VERSION 1.0
class Solution {
public:
    int uniquePaths(int m, int n) {
       vector<vector<int>> memo(n,vector<int>(m,0));
		for (int i = 0; i < m; i++)
			memo[0][i] = 1;
		for (int i = 0; i < n; i++)
			memo[i][0] = 1;
		for (int i = 1; i < n; i++)
			for (int j = 1; j < m; j++)
			{
				if (memo[i][j] == 0)
					memo[i][j] = memo[i - 1][j] + memo[i][j - 1];
				else
					continue;
			}
		return memo[n - 1][m - 1];
    }
};
